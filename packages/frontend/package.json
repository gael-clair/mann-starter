{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "doc": "typedoc ./src",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:ci": "ng lint --format junit > ./reports/lint.xml",
    "lint:style": "stylelint \"src/app/**/*.scss\" \"src/styles.scss\"",
    "lint:style:fix": "stylelint \"src/app/**/*.scss\" \"src/styles.scss\" --fix",
    "lint:style:ci": "stylelint \"src/app/**/*.scss\" \"src/styles.scss\" --custom-formatter \"node_modules/stylelint-junit-formatter\" > ./reports/style.xml",
    "test": "jest --config ./test/spec/jest.config.js",
    "test:watch": "jest --config ./test/spec/jest.config.js --watch",
    "test:cov": "jest --config ./test/spec/jest.config.js --coverage ",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/spec/jest.config.js --runInBand",
    "test:ci": "jest --config ./test/spec/jest.config.js --ci --runInBand --coverage",
    "e2e": "jest --config ./test/e2e/jest.config.js",
    "e2e:watch": "jest --config ./test/e2e/jest.config.js --watch",
    "e2e:cov": "concurrently --success first -k -c \"green,yellow\" -n \"Serve,Tests\" \"yarn start --configuration=e2e\" \"wait-on http-get://localhost:4200/ && jest --config ./test/e2e/jest.config.js --coverage && remap-istanbul --input ./coverage/e2e/coverage-puppeteer-istanbul.json --type html --output ./coverage/e2e/html-report\"",
    "e2e:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/e2e/jest.config.js --runInBand",
    "e2e:ci": "concurrently --success first -k -c \"green,yellow\" -n \"Serve,Tests\" \"ng serve --configuration=e2e\" \"wait-on http-get://localhost:4200/ && jest --config ./test/e2e/jest.config.js --ci --runInBand --coverage && remap-istanbul --input ./coverage/e2e/coverage-puppeteer-istanbul.json --output ./coverage/e2e/coverage-remaped.json\""
  },
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/cdk": "^9.0.1",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/material": "^9.0.1",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@hapi/joi": "^17.1.0",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@types/expect-puppeteer": "^4.4.0",
    "@types/jest": "^25.1.2",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/node": "^12.11.1",
    "@types/puppeteer": "^2.0.0",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jest-preset-angular": "^8.0.0",
    "jest-puppeteer": "^4.4.0",
    "jest-puppeteer-istanbul": "^0.5.0",
    "lint-staged": "^10.0.7",
    "ngx-build-plus": "^9.0.6",
    "prettier": "^1.19.1",
    "protractor": "~5.4.3",
    "puppeteer": "^2.1.1",
    "remap-istanbul": "^0.13.0",
    "stylelint": "^13.2.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-junit-formatter": "^0.2.2",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "ts-node": "~8.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.16.10",
    "typedoc-hopper-theme": "^0.0.2",
    "typescript": "~3.7.5",
    "wait-on": "^4.0.0"
  },
  "workspaces": {
    "nohoist": [
      "stylelint-junit-formatter",
      "@types/**",
      "@angular/material",
      "typedoc-hopper-theme"
    ]
  }
}
